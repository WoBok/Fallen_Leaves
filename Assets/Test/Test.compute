#pragma kernel CSMain

struct Pos {
    float x;
    float y;
    float z;
};

StructuredBuffer<float3> Positions;
RWStructuredBuffer<float3> ChangedPositions;

[numthreads(128, 1, 1)]
void CSMain(uint3 id : SV_DispatchThreadID) {
    float3 p = Positions[id.x];
    p.y += id.x;
    ChangedPositions[id.x] = p;
}